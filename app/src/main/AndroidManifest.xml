<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="tk.kolyanov.reminder" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".Retrieve.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Create.AddActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name=".Retrieve.RetrieveActivity"
            android:label="@string/title_activity_retrive" >
        </activity>
        <activity
            android:name=".Update.UpdateActivity"
            android:label="@string/title_activity_update" >
        </activity>
    </application>

</manifest>
